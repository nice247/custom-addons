<odoo>
    <record id="property_wizard_view_form" model="ir.ui.view">
        <field name="name">property wizard form</field>
        <field name="model">property.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="property_id" invisible="1"/>
                    <field name="status"/>
                    <field name="reason"/>
                </group>
                <footer>
                    <button string="Confirm" type="object" name="action_confirm" class="btn-primary"/>
                    <button string="Cancel" spetial="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
    <record id="property_change_wizard_action" model="ir.actions.act_window">
        <field name="name">Change State</field>
        <field name="res_model">property.wizard</field>
        <field name="target">new</field>
        <field name="view_mode">form</field>
    </record>
</odoo>